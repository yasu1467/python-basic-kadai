        "# 消費税を加えた金額を計算する関数\n",
        "def calc_tax_included(price, tax_rate):\n",
        "    # 消費税を計算\n",
        "    tax = price * tax_rate / 100\n",
        "    # 税込み金額を返す\n",
        "    return price + tax\n",
        "\n",
        "# 引数を設定（商品金額110円、消費税10%）\n",
        "result = calc_tax_included(110, 10)\n",
        "\n",
        "# 結果を表示\n",
        "print(result)\n"
      ]
    }
